{
    "name": "Bagley Bot host",
    "version": "1.0",
    "slug": "raspap", 
    "description": "Cette addon vous permttra de lancer l'hostage du bot Discord Bagley.",
    "url": "https://github.com/williamhelle/Hassio-Addons/",
    "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
    "startup": "application",
    "arch": ["armv7"],
    "map": ["config:rw"],
    "ports": { 
      "80/tcp": 4583
    },
    "ports_description": { 
      "80/tcp": "Web interface" 
    },
    "boot": "manual",
    "host_network": true,
    "hassio_api": true,
    "hassio_role": "default",
    "auth_api": true, 
    "privileged": ["NET_ADMIN"]
  }
